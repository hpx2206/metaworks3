<%
if(value!=null){
	console.debug(value);
%>
	<div id="actionButton">
		<%=methods.save.here()%>

<%
	if(value.created){
%>		
		<%=methods.dropTable.here()%>
<%
	} else {
%>
		<%=methods.createTable.here()%>
<%
	}
%>			
		<%=methods.generateDDL.here()%>
		<%=methods.generateDAO.here()%>
				
	</div>
	
	<div id="entityInfo">
		<table>
			<tr>
				<td>Entity Name</td>
				<td><%=fields.entityName.here()%></td>
			</tr>
		</table>
	</div>
				
	<div id="entityField">
		Columns<br />
		<table>
			<tr>
				<td width="200px">Name</td>
				<td width="140px">Datatype</td>
				<td width="70px">Length/Set</td>

				<td width="60px">Allow Null</td>
				<td width="200px">Default</td>
				<td width="200px">Comment</td>
				<td width="300px">&nbsp;</td>
				<td width="30px">&nbsp;</td>
			</tr>
		</table>
<%
	if(value.entityFields != null){
		for(var i=0; i<value.entityFields.length; i++){		
%>
		<%=mw3.locateObject(value.entityFields[i], value.entityFields[i].__className)%>
<%
		}	
	}
%>		
		<%=fields.newEntityField.here()%>
	</div>
<%
}
%>