<%
	if(value){
%>
	<%if(value.projectNode){
		if(mw3.how == 'perspectivePanel'){%>
			<ul>
				<% 
				
				for (var i=0; i<value.projectNode.length; i++){
			
					if(typeof options == 'undefined') 
						options = objectMetadata;
					
					options['index'] = i;
					options['htmlTag'] =  'li';
						
				%>		
					
					<%=mw3.locateObject(value.projectNode[i], value.projectNode[i].__className, null, options, {objectId: value.__objectId, name: 'projectNode[' + i + ']'})%>	
				<% 
				}
				%>
			</ul>
			<ul>
                <li>
                    <a href="#" onClick="<%=methods.addProject.caller()%>" class="add_new_label"><span class="glyphicon glyphicon-plus-sign" style="top:3px"></span>새프로젝트</a>
                    <div class="clear"></div>
                </li>
            </ul>
		<% }else{ %>
				<ul class="project_items">
				<% 
				
				for (var i=0; i<value.projectNode.length; i++){
			
					if(typeof options == 'undefined') 
						options = objectMetadata;
					
					options['index'] = i;
					options['htmlTag'] =  'li';
						
				%>		
					
					<%=mw3.locateObject(value.projectNode[i], value.projectNode[i].__className, null, options, {objectId: value.__objectId, name: 'projectNode[' + i + ']'})%>	
				<% 
				}
				%>
			</ul>
		<% } %>
	<% } %>
<%
	}
%>