
<%
	if(value){
%>
	<table >
		<tbody>
		<tr>
		
<%
				var nodeStatus = "opened";
				if(!value.close)
					nodeStatus = "closed";

				if(mw3.when != 'read'){
%>			
			<td id="mm_LineL"></td>
			<td id="wfnode_<%=objectId%>" class="wfNode" style="margin:3px 0; position:relative">
				
				<!-- opened, closed -->
				<div class="view_box4" id="hiddenReply" style="display: block; margin:7px 0 7px 0;">
					<div class="guide"><span class="tl"></span><span class="tr"></span></div>
					<div class="cont"">
					
						<table border="0" cellspacing="0" cellpadding="0" width="100%">
						<tr>
							<td class="<%=nodeStatus%>"  onclick="<%=value.close?methods.expand.caller():methods.collapse.caller()%>"><span>&nbsp;</span></td>
							<td>
								<span class="wfNodeText" style="width:100%;">
					<%	if(value.type=='img'){		
		%>
								<img src="<%=value.name%>">
								<input type="text" id="wfnode_content_<%=objectId%>" style="height:1px ; border:none;background:transparent" onkeydown="mw3.getFaceHelper('<%=objectId%>').keydown(this)" onkeyup="mw3.getFaceHelper('<%=objectId%>').keyup(this);">
		<%					
				
				
						}else if(value.type=='iframe'){		
		%>
								<iframe src="<%=value.name%>"></iframe>
								<input type="text" id="wfnode_content_<%=objectId%>" style="height:1px ; border:none;background:transparent" onkeydown="mw3.getFaceHelper('<%=objectId%>').keydown(this)" onkeyup="mw3.getFaceHelper('<%=objectId%>').keyup(this);">
		<%					
						
						
						}else{ %>
								<input type="text" id="wfnode_content_<%=objectId%>" style="width:100% ; border:none;background:transparent" style="border:0; font-family: 'Helvetica Neue', Arial, Sans-serif; font-size: 12px; font-weight: normal;background:transparent"" value="<%=value.name ? value.name : ''%>" onkeydown="mw3.getFaceHelper('<%=objectId%>').keydown(this)" onkeyup="mw3.getFaceHelper('<%=objectId%>').keyup(this);">
		<%				}%>


								</span>
							</td>
						
						<% if(value.linkedInstId){%><td><span class="wfLink" onclick="<%=methods.linkInstance.caller()%>"></span></td><%}%>
						
						</tr>
						</table>
					</div>
					<div class="guide"><span class="lb"></span><span class="rb"></span></div>
				</div>
					
				<div id="wfinfo_<%=objectId%>"></div>
			</td>			
<%
				}else{						
%>
			<td id="mm_LineL"></td>
			<td id="wfnode_<%=objectId%>" class="wfNode" style="margin:3px 0">
				<!-- opened, closed -->
				<div class="view_box4" id="hiddenReply" style="display: block; margin:7px 0 7px 0;">
					<div class="guide"><span class="tl"></span><span class="tr"></span></div>
						<div class="cont"">
							<table border="0" cellspacing="0" cellpadding="0">
								<tr>
									<td class="<%=nodeStatus%>"  onclick="<%=value.close?methods.expand.caller():methods.collapse.caller()%>"><span>&nbsp;</span></td>
									<td>
										<span class="wfNodeText">
											<input type="text" id="wfnode_content_<%=objectId%>" style="width:100% ; border:none;background:transparent" style="border:0; font-family: 'Helvetica Neue', Arial, Sans-serif; font-size: 12px; font-weight: normal" value="<%=value.name ? value.name : ''%>" readonly>
										</span>
									</td>
								</tr>
							</table>
						</div>
					<div class="guide"><span class="lb"></span><span class="rb"></span></div>
				</div>
				<div id="wfinfo_<%=objectId%>"></div>
			</td>
<%
				
				
				}
			
			
				if(!value.close){			
	%>
		<td id="mm_LineR"></td>
		<td>
			<%=fields.childNode.here()%>
		</td>
	<%			
				}
%>

		</tr>
		</tbody>
	</table>
<%			
	}
%>		