<%
	if(value){
		if(mw3.when == 'new' || mw3.when == 'edit'){
%>
	<div id="process">
		<dl>
			<dd>alias : <%=value.defId%></dd>
			<dd>name : <%=value.name%></dd>
		</dl>
	</div>
	
	<div id="editiconpop">
		<%=fields.iconColor.here()%>
		
		<dl>
			<dt>아이콘선택</dt>
			<dd style="width:230px;">        
				<%=fields.iconFile.here()%>          
			</dd>
			<dd style="width:230px; padding-top:3px;">
				<span style="color:#F60">* png 파일만 * <a href="http://www.iconfinder.com/" target="_blank"> 아이콘 다운로드 바로가기</a></span> 
			</dd>
		</dl>
		<div></div>
	</div>
	<div id="btnline" style="border-top:1px solid #E3E8F2; padding-top:8px;">
		<ul>			
			<li><a id="wBtn" href="javascript:<%=methods.close.caller()%>"><span>취소</span></a></li>			
<%
			if(!mw3.isHiddenMethod(methods.save)){
%>						
			<li><a id="wBtn" href="javascript:<%=methods.save.caller()%>"><span>저장</span></a></li>			
<%
			}
%>						
		</ul>
	</div>
<%		
		}else{
%>


   	<div class="editicon" onclick="<%=methods.modify.caller()%>"></div>
    <a href="javascript:<%=methods.initiate.caller()%>"><img alt="" src="images/processMap/iconDefault.png" width="40" height="40"></a>
    <a href="#"><%=value.name%></a>



	
	<!--<%=value.defId%>,<%=value.iconFile.uploadFilePath%>-->
<%
		}
	}
%>	