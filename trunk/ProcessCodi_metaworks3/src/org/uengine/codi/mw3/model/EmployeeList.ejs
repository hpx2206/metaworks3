<%
	if(value){
%>
		<ul>
<%
		for (var i=0; i<value.employee.length; i++){
			if(typeof options == 'undefined') 
				options = objectMetadata;
		
			options['index'] = i;
			options['htmlTag'] =  'li';
%>
	<%=mw3.locateObject(value.employee[i], value.employee[i].__className, null, options)%>	
<%
			mw3.addBeanProperty(value.__objectId, '.employee[' + i + ']');
		}
%>
		</ul>
<%
	}
%>