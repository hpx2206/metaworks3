<%if(value.statusPreview == 'converting'){%>
	<li></li>
	<li><b><i>미리보기 작업중...</i></b></li>
	<progress id="convertProgressBar"></progress>
	<li></li>
<%}%>
<%if(value.convertingResult != 'fail'){%>
	<span>
		<div class="workItem_tabs">
			 <ul>
				<li class="current">
					<a href="#" onClick="<%=methods.viewAsPDF.caller()%>"><span>PDF로 보기</span></a>
				</li>	
				<%if(value.pageCount != null){%>
			 	<li>
			 		<a href="#" onClick="<%=methods.viewAsImages.caller()%>"><span>이미지로 보기</span></a>
			 	</li>
				<%}%>
			 </ul>
		</div>
		<%if(value.mimeType != null && value.mimeType == 'image'){%>
			<%if(value.pageCount != null){%>
				<%for(i=0; i<value.pageCount; i++){%>
				<table>
					<tr>
						<td> <img width="100%" src="preview/<%=value.taskId%>.pdf_<%=i%>.jpg"/></td>
					</tr>
					<tr>
						<td align="center" style="padding:5px 0 20px 0; font-weight:bold">< Page <%=i+1%> ></td>
					</tr>
				</table>
				<%}%>
			<%}%>
		<%}else{%>
			<iframe src="preview/<%=value.taskId%>.pdf" frameborder="0" scrolling="no" width="100%" height="300px" />
		<%}%>
	</span>
<%}%>