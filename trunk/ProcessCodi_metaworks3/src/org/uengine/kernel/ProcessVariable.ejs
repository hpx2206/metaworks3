<select>

<%
	

	var processDesigner = mw3.getAutowiredObject('org.uengine.codi.mw3.webProcessDesigner.ProcessDesignerWebContentPanel');

	if(processDesigner && processDesigner.roleMap){
		for(var mapId in processDesigner.roleMap){
			var role = processDesigner.roleMap[mapId];
			
			var keyValue = mw3.getObjectKeyValue(role);
			var nameValue = mw3.getObjectNameValue(role);
			
			if(keyValue && nameValue){
%>
	<option value="<%=keyValue%>"> [Role]<%=mw3.localize(nameValue)%>
<%			
			}
		}
	}
%>

</select>