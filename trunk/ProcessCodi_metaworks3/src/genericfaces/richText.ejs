<%  
	 if(value == null){
	 	value = "";
	 }
	
	var cols = descriptor.getOptionValue('cols');
	var rows = descriptor.getOptionValue('rows');
	
	if(cols==null) cols = 50;
	if(rows==null) rows = 50;
	
	if(mw3.when == mw3.WHEN_EDIT || mw3.when == mw3.WHEN_NEW){
%>
	<textarea id="ckEditor_<%=objectId%>" name="ckEditor_<%=objectId%>" cols="<%=cols%>" rows="<%=rows%>">
		<%=value%>
	</textarea>
<%

	}else{
%>
	 "<%=value%>"
<%
	}
%> 