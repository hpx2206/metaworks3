
<%
if(value){

%>

	<div id="searchbox" style="border-bottom:1px solid #9EBFC4"> 
        <!--ui object -->
        <ul>
            <li>
                <div class="mp_sub_pic">
                	<%=fields.logoFile.here({when:'image', htmlAttr:{style:'width: 150px; height: 150px; border:1px solid #ccc; background:url(images/waveStyle/bg_thmb.gif) center no-repeat;'}})%>
                </div>


                <div class="mp_sub_rightBox">
                	<dl class="mp_sub_dsc1">
                    	<dt style="font-size:16px;"><%=fields.projectName.here()%></dt>
                        <dd><span class="mp_sub_tit"><%=mw3.localize("$project.domain")%> : </span><span class="mp_sub_rst"><%=value.domainName%></span></dd>
                        <dd><span class="mp_sub_tit"><%=mw3.localize("$ProjectDescription")%> : </span><span class="mp_sub_rst"><%=value.description%></span></dd>
                                   
                        <%
							if(value.type == "svn"){
						%>
								<dd><span class="mp_sub_tit"><%=mw3.localize("$project.svn")%> : </span><span class="mp_sub_rst"><%=value.svn%></span></dd>       
						       
						<%
						}
						%>
                    </dl>                    
                </div>
                
                <div>
					<%=methods.require.here()%>
					<%=methods.manageCommitter.here()%>
					<%=methods.server.here()%>
					<%=methods.distribute.here()%>
					<%=methods.releaseProject.here()%>
					<div class="clear"></div>
				</div>
            </li>
        </ul>
    </div>	


<%
}
%>