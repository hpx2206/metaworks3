<ul class="process_map">

<%
	var icon = "images/processMap/empty.png";
	var box = "folder";
	
	if (value.type == "process"){
		box = "white";	
		icon = "images/waveStyle/process.gif";
	} else if (value.type == "folder"){
		box = "folder";	
		icon = "images/processMap/empty.png";
	}

	if(value!=null){
		if(value.type == 'folder'){
%>

	<div class="fdiv" id="<%=box%>">
	   <div class="editicon"></div>
	    <a href="#"><img alt="" src="<%=icon%>" width="40" height="40"></a>
	    <a><%=value.name%></a>
	</div>

<%
			if(value.child){
%>		
	<div style="position:absolute; background:url(images/processMap/rarrowup.png) top left no-repeat; width:22px; height:11px; margin:120px 0 0 25px; z-index:1000;"></div>
	<div style=" box-shadow:5px 5px 20px  0 #ccc; border-radius: 10px; -moz-border-radius:10px;-webkit-border-radius:10px; background:#fff; border:1px solid #ccc; padding:5px 0 15px 0px; display:block; position:absolute; width:400px; margin-top:130px;  z-index:999;">
	<div><%=fields.child.here({when: mw3.when})%></div>
	<div style="clear:both;"></div></div>

<%
			}
	
		}else{
%>
	<div class="fdiv" id="<%=box%>" onClick="mw3.call(<%=objectId%>, 'appendProcessMap')">
		<div class="editicon"></div>
        <a href="#"><img alt="" src="<%=icon%>" width="40" height="40"></a>
        <a><%=value.name%></a>
    </div>

<%
	  	}
	}
%>

</ul>