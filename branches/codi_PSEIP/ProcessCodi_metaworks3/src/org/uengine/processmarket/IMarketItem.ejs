<%
var marketList="main";

if(value!=null){
	value.logoImageFile.mimeType='image/png';
	
	if(mw3.when==mw3.WHEN_VIEW || !value.isDeleted){
%>
	
	<%if(marketList=="main"){%>
	<div style="float:left; width:220px; background:#F8F8F8;height:215px; padding:5px; border-bottom:1px solid #C6C6C6; border-right:1px solid #C6C6C6; position:relative">
		<div>
			<img src="https://lh5.googleusercontent.com/gQwmJMq84b6uf_s2XByxMIrbICy4f26mLwHVqTxOhjHOCjCQuh71Eln6yZQVzxbLTecIVIbc1A=s220-h140-e365" width="220" height="140" />
			<%=fields.logoImageFile.here({when:'image'})%>
		</div>
		<div>
			 <b><%=value.itemName%></b> (ver <%=value.version%>)
		</div>

		<div  style="line-height:18px; margin:3px 0px;" class="badge">&nbsp;<%=value.price > 0 ? ' '+value.price + '원 ' : ' 무료 '%>&nbsp;</div>
		<div class="clear"></div>
		<div>
			<span style="background:url(images/waveStyle/checkbox-red.png) no-repeat 0 -19px; width:17px ; height:20px; float:left;"></span>
