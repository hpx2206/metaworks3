
<%
if(value){

%>

	<div id="searchbox" style="border-bottom:1px solid #9EBFC4"> 
        <!--ui object -->
        <ul>
            <li>
                <div class="mp_sub_pic project_img">
                	<%=fields.logoFile.here({when:'image', htmlAttr:{style:'width: 150px; height: 150px; line-height: 150px;border:1px solid #ccc; background:url(images/waveStyle/bg_thmb.gif) center no-repeat;'}})%>
                </div>


                <div class="mp_sub_rightBox">
                	<dl class="mp_sub_dsc1">
                    	<dt style="font-size:16px;"><%=fields.projectName.here()%></dt>
                    	<dd><span class="mp_sub_tit"><%=mw3.localize("$Alias")%> : </span><span class="mp_sub_rst"><%=value.projectAlias%></span></dd>
                        <dd><span class="mp_sub_tit"><%=mw3.localize("$ProjectDescription")%> : </span><span class="mp_sub_rst"><%=value.description%></span></dd>
                                   
                        <%
							if(value.type == "svn"){
						%>
								<dd><span class="mp_sub_tit" style="float:left; width:auto"><%=mw3.localize("$project.svn")%></span>
								<span style="float:left; position:relative; top:0px; cursor:pointer;"><img src="images/processMap/wheel.png" onClick ="<%=methods.manageCommitter.caller()%>" /></span>
								<span style="float:left"> : </span>
								<span class="mp_sub_rst"><textarea style="border:none; background: #C9E2FC;overflow: hidden;position: relative;top: -5px;width: 60%;"><%=value.svn%></textarea></span></dd>
						       					
						    <div> <%=methods.manageCommitter.here()%></div>
						<%
						}
						%>
                    </dl>                    
                </div>
                <div class="clear"></div>
                <div id="searchbox" style="border-bottom:1px solid #9EBFC4;">
	                <div class="positionRight" style="padding:5px 0">
						<%=methods.metadata.here()%>
						<%=methods.server.here()%>
						<%=methods.distribute.here()%>
						<%=methods.releaseProject.here()%>
						<%
							if(value.type == "svn"){
						%>
								<%=methods.hudson.here()%>
						<%
						}
						%>
						<div class="clear"></div>
					</div>
				</div>
				<div class="clear"></div>
            </li>
        </ul>
    </div>	


<%
}
%>