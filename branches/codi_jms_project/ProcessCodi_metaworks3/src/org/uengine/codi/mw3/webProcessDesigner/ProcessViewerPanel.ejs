<%
    if( value ){
        if( mw3.how == 'find' ){
%>
            <div style="_display:inline-block;width:100%; height:100%;">
                <div style="height:100%;float:left;width:30%; border-right:1px solid #B4B3B5; background:#F6F6F6;">
                    <%= fields.processViewNaivgator.here() %>
                </div>
                <div style="height:100%;float:left;width:69%;">
                    <%= fields.processViewPanel.here() %>
                </div>
            </div>
            <div style="position:absolute; right:10px; bottom:10px; z-index:1000;">
                <a id="wBtn" href="#" onClick="<%=methods.saveLink.caller()%>"><span><%=mw3.localize("$Save")%></span></a>
            </div>
<%
        }else{
%>
                     <%= fields.processViewPanel.here() %>
                        <div style="position:absolute; right:10px; bottom:10px; z-index:1000;">
                <%
                    if( value.viewType == 'definitionView' ){
                %>
                        <a id="wBtn" href="#" onClick="<%=methods.openLink.caller()%>"><span><%=mw3.localize("$Open")%></span></a>
                <%
                    }else{
                %>
                        <a id="wBtn" href="#" onClick="<%=methods.openLink.caller()%>"><span><%=mw3.localize("$Open")%></span></a>
                        <a id="wBtn" href="#" onClick="<%=methods.removeLink.caller()%>"><span><%=mw3.localize("$ProcessReselect")%></span></a>
                <%
                    }
                %>
          
            </div>
<%
        }
%>
<%
    }
%>