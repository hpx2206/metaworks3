<% if(value) { 
    
    if(mw3.when == 'enterprise') { %>
    
        <ul id="green_and_orange_step_menu">
            <li class="first inprogress">STEP3<em>Enterprise Information</em></li>
            <li class="last">STEP4<em>Service</em><span></span></li>
        </ul>
    
        <div class="form_table">
            <table border="1" cellspacing="0" summary="">
                <tbody>
                <tr>
                    <th scope="row">이름</th>
                    <td>
                        <div class="item">
                            <%=fields.enterpriseName.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">대표자</th>
                    <td>
                        <div class="item">
                            <%=fields.representative.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">사업분야</th>
                    <td>
                        <div class="item">
                            <%=fields.businessPart.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">주소</th>
                    <td>
                        <div class="item">
                            <%=fields.address.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">전화번호</th>
                    <td>
                        <div class="item">
                            <%=fields.number.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">회사설립년도</th>
                    <td>
                        <div class="item">
                            <%=fields.foundation.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                <tr>
                    <th scope="row">해당부문 종사기간</th>
                    <td>
                        <div class="item">
                            <%=fields.carryon.here({when:'new'}) %>
                        </div>
                    </td>
                </tr>
                
                </tbody>
            </table>
        </div>
        
        <div id="btnline" style="padding-top:8px; border-top:1px solid #ccc;">
            <ul>    
                <li><a id="wBtn" href="#" onClick="<%=methods.inputNextService.caller()%>"><span><%=mw3.localize('$Next')%></span></a></li>
            </ul>
        </div>
        
    <% } else if(mw3.when == 'service') { %>
        <%=fields.enterpriseService.here()%>
        <div id="btnline" style="padding-top:8px; border-top:1px solid #ccc;">
            <ul>    
                <li><a id="wBtn" href="#" onClick="<%=methods.inputNextPatent.caller()%>"><span><%=mw3.localize('$Next')%></span></a></li>
            </ul>
        </div>
            
    <% } else if(mw3.when == 'patent') { %>
        <%=fields.enterprisePatent.here()%>
        <div id="btnline" style="padding-top:8px; border-top:1px solid #ccc;">
            <ul>    
                <li><a id="wBtn" href="#" onClick="<%=methods.inputNextSurvey.caller()%>"><span><%=mw3.localize('$Next')%></span></a></li>
            </ul>
        </div>
        
        
    <% } else { %>
        <%=fields.selfTestSurvey.here()%>
        <div id="btnline" style="padding-top:8px; border-top:1px solid #ccc;">
            <ul>    
                <li><a id="wBtn" href="#" onClick=""><span><%=mw3.localize('$Next')%></span></a></li>
            </ul>
        </div>
        
    <% } %>
<% } %>