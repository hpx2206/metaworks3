<% if(value!=null){

	if(mw3.where=="instanceNavigator"){%>
	
<div style="cursor:pointer" onclick="<%=methods.flowchart.caller()%>"><%=value.defName%></div>	
	
	<%
		mw3.where="anywhere";
	
	}else{%>
	
<table width="100%" border="0" cellspacing="0" cellpadding="0" class="tbl_type">						
	<tbody>
	<tr  class="q <%=value.firstDueTimeTerm ? (value.firstDueTimeTerm>=0 ? (value.firstDueTimeTerm>=3 ? "" : "plus" + value.firstDueTimeTerm ) : "minus" )  : "" %>" onClick="<%=methods.detail.caller()%>" style="cursor:pointer;">
		<td class="first_td">
			<div class="worklistuser">
					<dl>					                		
						<dd><%=fields.initiator.here() %></dd>
						<dd><%=fields.currentUser.here() %></dd>
						<div class="clear"></div>					                	
					</dl>	
			</div>				
		</td> 
		<td>									
			<ul class="lst_type">										
				<li class="trigger"><span class="<%= value.defId==null?'type_ping':'type_process' %>"></span><a href="#middle-center"><%=fields.name.here()%></a></li>								
			</ul>								
		</td>
		<td class="last_td"><%=fields.startedDate.here()%></td>
	</tr>
	
</table>


	<%}%>


<%}%>
