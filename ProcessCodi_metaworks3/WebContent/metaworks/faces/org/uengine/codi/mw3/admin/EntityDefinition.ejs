<%
if(value!=null){
%>
	<div id="entityInfo">
		<table>
			<tr>
				<td>Entity Name</td>
				<td><%=fields.entityName.here()%></td>
			</tr>
		</table>
	</div>
				
	<div id="entityField">
		<table>
			<tr>
				<td width="200px">Name</td>
				<td width="140px">Datatype</td>
				<td width="70px">Length/Set</td>

				<td width="60px">Allow Null</td>
				<td width="200px">Default</td>
				<td width="200px">Comment</td>
				<td width="300px">&nbsp;</td>
				<td width="30px">&nbsp;</td>
			</tr>
		</table>
		<%=fields.entityFields.here()%>
		<%=fields.newEntityField.here()%>
	</div>	
	
	<div id="actionButton">
		<%=methods.generateDDL.here()%>
		<%=methods.save.here()%>
		<%=methods.createTable.here()%>
		<%=methods.generateDAO.here()%>
				
	</div>
	
	<%=fields.script.here()%>
<%
}
%>