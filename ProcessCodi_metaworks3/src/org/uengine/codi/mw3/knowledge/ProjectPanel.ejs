<%
	if(value){
%>
	<!--	
		<a id="wBtn" href="#" onClick="<%=methods.addProject.caller()%>"><span><%=mw3.localize('$CreateProject')%></span></a><div class="clear"></div><br>
	-->	
	
	<%if(value.projectNode){%>
		<ul class="fist_menu">
			<% 
			
			for (var i=0; i<value.projectNode.length; i++){
		
				if(typeof options == 'undefined') 
					options = objectMetadata;
				
				options['index'] = i;
				options['htmlTag'] =  'li';
					
			%>		
				
				<%=mw3.locateObject(value.projectNode[i], value.projectNode[i].__className, null, options)%>	
			<% 
				mw3.addBeanProperty(value.__objectId, '.projectNode[' + i + ']');
			}
			%>
			
		</ul>
	<% } %>
	
<%
	}
%>