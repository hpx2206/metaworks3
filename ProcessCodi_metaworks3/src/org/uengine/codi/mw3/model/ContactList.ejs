<%
	if(value){
	
	
		if(value.invitation && value.id != 'contacts_local'){
			%><%=fields.invitation.here()%><%
		}else{
%>

		<%=fields.contacts.here()%>
		<div class="user" >
			<div style="float:left; width:33px; height:33px; font-size:24px; font-weight:bold; line-height:31px; text-align:center; margin-top:7px" class="user_focus">+</div>
		</div>
	
			<% if(!value.selectedMore) { %>
				
				<% if(value.contactCount >= 10) { %>
					
					<span class="west_more_btn" style="top:-20px;"><a onClick="<%=methods.moreView.caller()%>" href="#" style="display: inline-block"><%=mw3.localize('$More')%></a></span>
				
				<% } %>
			
			<% } else { %>
				
				<span class="west_more_btn" style="top:-20px;"><a onClick="<%=methods.simpleView.caller()%>" href="#" style="display: inline-block"><%=mw3.localize('$SimpleView')%></a></span>
			<% } %>
<%
		}
	}
%>	