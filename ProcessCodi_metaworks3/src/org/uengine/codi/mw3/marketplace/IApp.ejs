	
<%
if(value){

var session = mw3.getAutowiredObject("org.uengine.codi.mw3.model.Session");

var company = session.company[0];
var when = value.metaworksContext.when;


	if(when == 'detailList'){

%>
	<div class="detailView_app_outer">
		<div class="detailView_app">
			<div class="topTitle_app"><%=value.appName%></div>
			<div class="byCompany_app">by <%=company.comName%></div>
			
			<div class="clear"></div>
		</div>
		<div class="fixed_img_row" >
	   	  <ul>	
		     <li style="border:0; margin-bottom:30px;"> 
	        	<table width="100%" border="0" cellspacing="0" cellpadding="0">
	                <tr>
	                    <td class="appFirstTd"><span class="thumb"> <%=fields.logoFile.here({when:'image'})%> </span> </td>
	                    <td class="appTitleTd">
	                    	<div> <%=value.simpleOverview%> </div>
							<div class="PricingDetail_app" style="float:left"> Pricing Detail : </div>
							<div style="float:left;margin:7px 5px"><%=value.pricing%> </div>
							<div class="clear"></div>
							<div class="PricingDetail_app"> Status > <%=value.status%> </div>
	                    </td>	 
	                    <td class="appBtnTd">
	                    	<% if(value.status == 'Published') { %>
	                    	
								<div class="badge" style="float:none;margin-top:2px;background:#679BD1;padding:3px 8px; width:80px"><a onClick="<%=methods.addApp.caller()%>"> Add it now </a></div>
							
							<% } %>
						</td>                 
	                </tr>
	            </table>
			 </li>
			</ul>
		</div>
	
		
		<div class="workItem_tabs">
			 <ul>
				<li class="current">
					<a href="#" onClick="mw3.getFaceHelper('<%=objectId%>').showDescription()"><span>Description</span></a>
				</li>	
			 	<li>
			 		<a href="#" onClick="mw3.getFaceHelper('<%=objectId%>').showVendor()"><span>Vendor</span></a>
			 	</li>
			 </ul>
		</div>
		<div class="clear"></div>
		
		<div id="tab_Vendor" style="display:none">
		
			<div class="fixed_img_row">
		   	  <ul>	
			     <li> 
		        	<table width="100%" border="0" cellspacing="0" cellpadding="0">
		                <tr>
		                    <td class="appFirstTd"><span class="thumb"> logo </span> </td>
		                    <td class="appTitleTd">
		                    	<div class="titleLine"> <strong><%=company.comName%></strong></div>
		                    	<div class="fullOverview"><%=company.description%></div> 
		                    	<div class="simpleOverview"><%=company.repMail%></div>
		                    </td>
		                </tr>
		            </table>
				 </li>
				</ul>
			</div>
		</div>

		<div id="tab_Description">
			<%=value.fullOverview%>
		</div>
	</div>
				
	
<%	
	
	}else if(when == 'newApps') {

%>
<div style="padding:7px 0; border-bottom:1px solid #e5e5e5;">
	<div style="width:50px; float:left; margin:4px;"> <%=fields.logoFile.here({when:'image'})%> </div>
	<div style="margin-left:50px;">  
	     	<div class="titleLine" style="margin:0px"><a href="#" onClick="<%=methods.detailListing.caller()%>"> <%=value.appName%> </a></div>
	     	<div class="newApp_pricing"> <%=value.pricing%> </div>  
	</div>  
	<div class="clear"></div>
</div>
<%
	
	}else {
	
		if(value.status != 'Request'){
	
%>

	<div class="fixed_img_row">
	   	  <ul>	
		     <li> 
	        	<table width="100%" border="0" cellspacing="0" cellpadding="0">
	                <tr>
	                    <td class="appFirstTd"><span class="thumb"> <%=fields.logoFile.here({when:'image'})%> </span> </td>
	                    <td class="appTitleTd">
	                    	<div class="titleLine"> <strong><a href="#" onClick="<%=methods.detailListing.caller()%>"> <%=value.appName%> </a></strong></div>
	                    	<div class="simpleOverview"><%=value.simpleOverview%></div>
	                    	<!--div class="fullOverview"><%=value.fullOverview%></div--> 
	                        <div class="PricingDetail_app" style="float:left"> Pricing Detail : </div>
							<div style="float:left;margin:7px 5px"><%=value.pricing%> </div>
							<div class="clear"></div>
	                        <div class="vendor"> service by <%=company.comName%></div>
	                    </td>
	                    <td class="appStatusTd"><b>Listing Status</b><br><%=value.status%> </td>
	                    <td class="appBtnTd">
	                    	<%
	                    	if(when == 'marketplaceHome' || when == 'searchForSearchBox' || when == 'searchForCategory'){
							
								if(value.status == 'Published') {
							%>
							
								<div class="badge" style="float:none;margin-top:2px;background:#679BD1;padding:3px 8px; width:80px"><a onClick="<%=methods.addApp.caller()%>"> Add it now </a></div>
							
							<%
								}else if(value.status == 'Approved'){
							
							%>
									<div class="badge" style="float:none;margin-top:2px;background:#679BD1;padding:3px 8px; width:80px"><a onClick="<%=methods.readyPublished.caller()%>"> Ready for publish </a></div>
								
								<%
								}
							
							}else if(when == 'myVendor'){
							
							%>
							
								<%if(value.status == 'Approved'){%>
									<div class="badge" style="float:none;margin-top:2px;background:#679BD1;padding:3px 8px; width:80px"><a onClick="<%=methods.readyPublished.caller()%>"> Ready for publish </a></div>
								<%}%>
								
								<div class="badge" style="float:none;margin-top:2px;background:#679BD1;padding:3px 8px; width:80px"><a onClick="<%=methods.editListing.caller()%>"> Edit Detail </a></div>
						
							<%
							}
							
							%>
						</td>
	                </tr>
	            </table>
			 </li>
			</ul>
		</div>	

<%	
		}
	
	}

}
%>