<%
    if( value && value.activityList ){
        for(var i in value.activityList){
            var activity = value.activityList[i];
           %>
           <%=mw3.locateObject(activity.activityView , activity.activityView.__className)%>
           <%
           mw3.addBeanProperty(objectId, "[" + i + "]");
        }
    }
    if( value && value.transitionList ){
        for(var i in value.transitionList){
            var transition = value.transitionList[i];
           %>
           <%=mw3.locateObject(transition.activityView , transition.activityView.__className)%>
           <%
           mw3.addBeanProperty(objectId, "[" + i + "]");
        }
    }
%>