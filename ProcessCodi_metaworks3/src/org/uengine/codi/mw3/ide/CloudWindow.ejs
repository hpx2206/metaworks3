<%
if (value != null){	
%>

<div class="top" style="background-color:yellow; height: 60px; overflow:hidden; position: absolute; min-width: 0px; min-height: 0px; max-width: 10000px; max-height: 10000px;  left: 0px; right: 0px; top: 0px;">
<%	
	for (var i=0; i<value.tabs.length; i++){
		var tabName = '';
		var tabMetadata = mw3.getMetadata(value.tabs[i].__className);
		
		if(tabMetadata.nameFieldDescriptor){

		}else{
			tabName = tabMetadata.displayName;
		}
		
		console.log(tabName);
		
		var tabOptions = {
			ejsPath : 'dwr/metaworks/org/uengine/codi/mw3/ide/CloudTab.ejs'
		};
		
		var tab = {
			__className : 'org.uengine.codi.mw3.ide.CloudTab',
			name : tabName
		};
%>
	<div id="top_<%=objectId%>_<%=i%>" onClick="mw3.getFaceHelper('<%=objectId%>').select('<%=objectId%>_<%=i%>');">
		<%=mw3.locateObject(tab, tab.__className, null, tabOptions)%>
	</div>
<%			
	}
%>                		
</div>

<div class="bottom" style="background-color:green;  top: 60px; overflow:auto; position: absolute; min-width: 0px; min-height: 0px; max-width: 10000px; max-height: 10000px;  left: 0px; right: 0px; bottom: 0px;">

<%
	for (var i=0; i<value.tabs.length; i++){
%>
	<div id="bottom_<%=objectId%>_<%=i%>" style="<%=(i>0)?'display:none':''%>">
		<%=mw3.locateObject(value.tabs[i], value.tabs[i].__className)%>
	</div>		
<%	
	}
%>

</div>

<%
}
%>