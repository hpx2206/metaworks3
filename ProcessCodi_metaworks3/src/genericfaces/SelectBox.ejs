<%
	if(mw3.when == mw3.WHEN_EDIT || mw3.when == mw3.WHEN_NEW){
%>
		<select class="form-control" name="selectbox_<%=objectId%>">
<%
	for(var i=0; i<descriptor.options.length; i++){
		var inputOption = '';
		if(value == descriptor.values[i])
			inputOption = ' selected="selected"';
		
%>
		<option value="<%=mw3.localize(descriptor.values[i])%>" <%=inputOption%>>
		<%=mw3.localize(descriptor.options[i])%> </option>
<%  
	}
}else{
	
	if(value != null){
		for(var i=0; i<descriptor.options.length; i++){
			if(value == descriptor.values[i]){
	%>
		<%=mw3.localize(descriptor.options[i])%>
	<%
			}
		}

	}
}
%>
