<%
	if(value == null && typeof value == 'undefined')
		return;
		
	var session = mw3.getAutowiredObject("org.uengine.codi.mw3.model.Session");
	var isTopic = (mw3.how == "topic");
	var isProject = (mw3.how == "project");
	var isDashboard = (mw3.how == "dashboard");
	var isApp = (mw3.how == "app");
	
	var height = 0;
	if(isTopic) {
		height = 115;
	}else if(isProject || isApp){
		height = 500;
	}else if(isDashboard){
		height = 38;
	}else{
		height = 68;
	}
	
%>

<% if(isDashboard){%>
	<div>
	<%=mw3.localize("$MyNews")%>
	</div>
	
	<span><a id="wBtn" onClick="<%=methods.loadOnDashboard.caller()%>" href="#"><%=mw3.localize("$MyNews.more")%></a></span>
	
	<div class="bottom" style="top: <%=height%>px; position: static; min-width: 0px; min-height: 0px; max-width: 10000px; max-height: 10000px;  left: 0px; right: 0px; bottom: 0px;">


				<%=value.instanceList ? fields.instanceList.here() : ""%>
				<%=value.scheduleCalendar ? fields.scheduleCalendar.here() : ""%>
				<%=value.knowledge ? fields.knowledge.here() : ""%>

	</div>
<% }else{%>
<div class="top" style="height: <%=height%>px; overflow:hidden; position: absolute; min-width: 0px; min-height: 0px; max-width: 10000px; max-height: 10000px;  left: 0px; right: 0px; top: 0px;">

	<%=isApp?fields.topicFollowers.here():''%>
	<%=isApp?fields.appInformation.here():''%>
	
	<%=isTopic?fields.topicFollowers.here():''%>
	<%=isProject?fields.topicFollowers.here():''%>
	<%=isProject?fields.projectInfo.here():''%>
	<%=methods.loadProjectInfo.here()%>
		            <div id="searchbox">
		                <div class="searchboxline">
		                	<div class="searchboxline2">
			                        <div class="newprocessbtn">
				                        <a href="#" onclick="<%=methods.newInstance.caller()%>">
				                        	<span id="method_<%=objectId%>_newInstance">
				                        		<div id="startBtn">
							                        <% if(mw3.how == "topic"){ 
								                        	var title = session.windowTitle;
															if( title != null && title != '' && title.length > 13){
																title = title.substring(0,13) + '...';
															}%>
														"<%= title %>"Ïóê
													<%}%> 
														<%=mw3.localize("$New")%>
												</div>
											</span>
										</a>
									</div>
		                        <div class="searchboxarea" id="method_<%=objectId%>_newInstance">
		                        	<%=fields.searchBox.here()%>
		                        </div>
		                        <div style="clear:both;"></div>
		                    </div>
		                </div>
		                <div style="clear:both;"></div>
		            </div>    
		            <% if(!isProject && !isApp){%>
			            <div class="toolbar">
		                    <ul>
		                        <li onclick="<%=methods.switchToInstanceList.caller()%>" <%=(value.instanceList && session.lastPerspecteType=='allICanSee') ? "class='toolbar_current'" : ""%>><span class="toolbarIcon"><img src="images/waveStyle/i_upcoming.png" border="0"></span><span><%=mw3.localize("$ComingUp")%></span></li>
		                        <li onclick="<%=methods.switchToInstanceListFollowing.caller()%>" <%=(value.instanceList && session.lastPerspecteType=='all') ? "class='toolbar_current'" : ""%>><span class="toolbarIcon"><img src="images/waveStyle/i_follow.png" border="0"></span><span ><%=mw3.localize("$Following")%></span></li>               
		                        <li onclick="<%=methods.switchToInstanceListMyToDo.caller()%>" <%=(value.instanceList && session.lastPerspecteType=='inbox') ? "class='toolbar_current'" : ""%>><span class="toolbarIcon"><img src="images/waveStyle/i_todo.png" border="0"></span><span><%=mw3.localize("$MyToDo")%></span></li>               
		                        <% if(value.useKnowledge){ %>
		                        <li onclick="<%=methods.switchToKnowledge.caller()%>" <%=value.knowledge ? "class='toolbar_current'" : ""%>><span class="toolbarIcon"><img src="images/waveStyle/i_know.png" border="0"></span><span id="method_<%=objectId%>_switchToKnowledge"><%=mw3.localize("$Strategic")%></span></li>               
		                     	<% } %>
		                        <li onclick="<%=methods.switchToScheduleCalendar.caller()%>" <%=value.scheduleCalendar ? "class='toolbar_current'" : ""%>><span class="toolbarIcon"><img src="images/waveStyle/i_gantt.png" border="0"></span><span><%=mw3.localize("$Calendar")%></span></li>
		                    </ul>
	                	</div>
                	<% }%>
				
					
                	
</div>
<div class="bottom" style="top: <%=height%>px; position: absolute; min-width: 0px; min-height: 0px; max-width: 10000px; max-height: 10000px;  left: 0px; right: 0px; bottom: 0px;">


				<%=value.instanceList ? fields.instanceList.here() : ""%>
				<%=value.scheduleCalendar ? fields.scheduleCalendar.here() : ""%>
				<%=value.knowledge ? fields.knowledge.here() : ""%>

</div>
<% }%>