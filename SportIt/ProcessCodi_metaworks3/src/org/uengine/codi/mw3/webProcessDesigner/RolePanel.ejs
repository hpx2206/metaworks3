<% if(value){ %>
    <% if(mw3.how == 'list'){ %>
    <!-- 켄버스의 참여자정의 메뉴를 더블 클릭시 나오는 리스트 -->
        <div>
                <table class="form_table2"> 
                    <tbody>
                    <tr>
                        <th>롤 이름</th>
                        <th>롤 설명</th>
                    </tr>
                    <%
                        if( value.roleList ){
                            for(var i=0; i < value.roleList.length; i++){
                                var roleName = value.roleList[i].name;
                    %>
                                <tr>
                                    <td class="rolePanel_td"><%= roleName %></td>
                                    <td class="rolePanel_td"><%= value.roleList[i].displayName != null ?  value.roleList[i].displayName.text : '' %></td>
                                </tr>
                    <%
                            }
                        }else{
                    %>
                                <tr>
                                    <td colspan="2">변수가 없습니다</td>
                                </tr>
                    
                   <% }%>
                   
                    </tbody>
                </table>
        </div>
     <% }else if(mw3.how == 'menu'){ %>
     <!-- 켄버스의 전체변수 설정 텝-->
            <%= fields.roleList.here() %>
     <% }%>
<% }%>